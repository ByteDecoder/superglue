{
  "name": "@jho406/breezy",
  "version": "0.1.4",
  "description": "Develop ReactJS apps the Rails Way",
  "main": "dist/breezy.js",
  "files": [
    "lib/assets/javascripts"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:all && npm run blade",
    "blade": "bundle exec blade runner",
    "export:rails":"npm run build:browser:src && cp build/browser/breezy.js ../breezy_rails/lib/assets/javascript/breezy.js" ,

    "build:src": "node_modules/coffeescript/bin/coffee -o build/src/ -c src/",
    "build:browser:src": "browserify src/index.coffee -o build/browser/breezy.js -t coffeeify --extension='.coffee'",
    "build:browser:test": "browserify test/src/test.coffee -o build/browser/test.js -t coffeeify --extension='.coffee'",
    "build:test": "node_modules/coffeescript/bin/coffee -o build/test/src -c test/src",
    "build:all": "npm run build:src && npm run build:browser:src && npm run build:browser:test && npm run build:test",
    "qunit": "npm run build:all && node_modules/qunit/bin/cli.js -c build/src/index.js -t build/test/src/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jho406/breezy.git"
  },
  "keywords": [
    "turbolinks",
    "browser",
    "pushstate"
  ],
  "author": "jho406",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jho406/breezy/issues"
  },
  "homepage": "https://github.com/jho406/breezy#readme",
  "dependencies": {
    "eventemitter3": "^2.0.3",
    "history": "^4.6.3",
    "superagent": "^3.5.2",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "browserify": "^14.4.0",
    "browserify-istanbul": "^2.0.0",
    "coffeeify": "^2.1.0",
    "coffeescript": "^1.12.7",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "qunit": "^1.0.0",
    "sinon": "^1.17.7",
    "superagent-mock": "^3.5.0",
    "superagent-prefix": "0.0.2"
  }
}
