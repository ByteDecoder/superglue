<%- attributes_list_with_timestamps.each do |attr|-%>
json.<%=attr%> @<%= singular_table_name %>.<%=attr%>
<%- end -%>

if notice
  json.notice notice
end

json.meta do
  json.index_path <%= plural_table_name %>_path
  json.edit_path edit_<%= singular_table_name %>_path(@<%= singular_table_name %>)
end
